.qingful-bbs-container {
  position: relative;
  width: 100%;
  height: 100%;
  background: #e6e6e6;
  font-family: "PingFangSC-Regular";
}
.qingful-bbs-container image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.qingful-bbs-container .partOne {
  position: relative;
  width: 100%;
  height: 240rpx;
}
.qingful-bbs-container .partOne .imgWrapper {
  width: 100%;
  height: 100%;
  background: lightblue;
}
.qingful-bbs-container .partOne .mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.2);
}
.qingful-bbs-container .partOne .con {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 40rpx 24rpx;
}
.qingful-bbs-container .partOne .con view {
  display: inline-block;
  vertical-align: middle;
}
.qingful-bbs-container .partOne .con view.userPhoto {
  width: 160rpx;
  height: 160rpx;
  border-radius: 10rpx;
  background: azure;
}
.qingful-bbs-container .partOne .con view:nth-child(2) {
  width: 494rpx;
  color: #fff;
  font-size: 26rpx;
  margin-left: 34rpx;
}
.qingful-bbs-container .partOne .con view:nth-child(2) view:nth-child(1) {
  font-size: 34rpx ;
  line-height: 48rpx;
}
.qingful-bbs-container .partOne .con view:nth-child(2) view:nth-child(2) {
  margin-left: 0;
}
.qingful-bbs-container .partOne .con view:nth-child(2) view:nth-child(2) text {
  display: inline-block;
  line-height: 38rpx;
  margin-top: 10rpx;
}
.qingful-bbs-container .userList .user {
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  margin-top: 20rpx;
  padding: 0rpx 24rpx;
}
.qingful-bbs-container .userList .user .name {
  position: relative;
  vertical-align: middle;
  padding-top: 20rpx;
  height: 110rpx;
}
.qingful-bbs-container .userList .user .name view {
  display: inline-block;
  color: #222;
}
.qingful-bbs-container .userList .user .name view:nth-child(2) {
  width: 210rpx;
  height: 100%;
  vertical-align: top;
  padding-top: 3rpx;
}
.qingful-bbs-container .userList .user .name view:nth-child(2) text {
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
  line-height: 42rpx;
}
.qingful-bbs-container .userList .user .name view:nth-child(2) text:nth-child(2) {
  font-size: 26rpx;
  color: #8a8a8a;
  line-height: 38rpx;
}
.qingful-bbs-container .userList .user .name .photo {
  width: 90rpx;
  height: 90rpx;
  margin-right: 20rpx;
  background: lightgray;
  border-radius: 50%;
}
.qingful-bbs-container .userList .user .name .concat {
  position: absolute;
  top: 50%;
  right: 24rpx;
  transform: translateY(-50%);
  width: 120rpx;
  height: 56rpx;
  border: 2rpx solid #ff5757;
  text-align: center;
  line-height: 56rpx;
  font-size: 26rpx;
  border-radius: 6rpx;
  color: #ff5757;
}
.qingful-bbs-container .userList .user .title {
  width: 100%;
  height: 46rpx;
  vertical-align: middle;
}
.qingful-bbs-container .userList .user .title text {
  display: inline-block;
  font-size: 32rpx;
  color: #222;
  font-weight: bold;
  text-align: center;
  line-height: 46rpx;
  vertical-align: middle;
}
.qingful-bbs-container .userList .user .title text:nth-last-child(2) {
  margin-right: 14rpx;
}
.qingful-bbs-container .userList .user .title .top {
  padding: 2rpx;
  width: 32rpx;
  height: 32rpx;
  border-radius: 6rpx;
  font-size: 22rpx;
  background-color: #ff5757;
  color: #fff;
  line-height: 32rpx;
  margin-right: 2rpx;
}
.qingful-bbs-container .userList .user .title .better {
  padding: 2rpx;
  width: 32rpx;
  height: 32rpx;
  font-size: 22rpx;
  border-radius: 6rpx;
  background-color: #08b7fc;
  color: #fff;
  line-height: 32rpx;
  margin-right: 14rpx;
}
.qingful-bbs-container .userList .user .con {
  margin-top: 20rpx;
}
.qingful-bbs-container .userList .user .con view {
  font-size: 28rpx;
}
.qingful-bbs-container .userList .user .con view:nth-child(1) {
  line-height: 40rpx;
  display: -webkit-box;
  overflow: hidden;
  white-space: normal!important;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.qingful-bbs-container .userList .user .con .imgList {
  width: 100%;
  margin: 20rpx 0rpx;
  box-sizing: border-box;
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
}
.qingful-bbs-container .userList .user .con .imgList .imgWrapper {
  display: block;
  font-size: 0;
  text-align: center;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  margin-right: 20rpx;
}
.qingful-bbs-container .userList .user .con .imgList .imgWrapper:nth-child(3n) {
  margin-right: 0;
}
.qingful-bbs-container .userList .user .con .imgList .imgWrapper image {
  width: 220rpx;
  height: 220rpx;
  background: #e6e6e6;
}
.qingful-bbs-container .userList .user .con .tagList {
  margin-bottom: 20rpx;
}
.qingful-bbs-container .userList .user .con .tagList text {
  margin-right: 20rpx;
  line-height: 48rpx;
  font-size: 26rpx;
  background-color: #efefef;
  padding: 6rpx 12rpx;
  color: #999;
  border-radius: 6rpx;
}
.qingful-bbs-container .userList .user .con .position {
  position: relative;
  width: 100%;
  color: #8a8a8a;
  font-size: 26rpx;
  vertical-align: middle;
}
.qingful-bbs-container .userList .user .con .position .icon {
  width: 22rpx;
  height: 22rpx;
  border-radius: 50%;
  vertical-align: middle;
  padding-left: 0;
}
.qingful-bbs-container .userList .user .con .position text {
  display: inline-block;
  font-size: 26rpx;
  line-height: 38rpx;
  color: #999;
  margin-right: 16rpx;
  vertical-align: middle;
}
.qingful-bbs-container .userList .user .con .position text:nth-last-child(1) {
  position: absolute;
  right: 0;
}
.qingful-bbs-container .userList .user .con .position .icon-address {
  margin-bottom: 14rpx;
}
.qingful-bbs-container .userList .user .btm {
  display: flex;
}
.qingful-bbs-container .userList .user .btm .operateTab {
  flex: 1;
  text-align: center;
  padding: 30rpx 0rpx;
}
.qingful-bbs-container .userList .user .btm .operateTab text {
  display: inline-block;
  margin-right: 12rpx;
  font-size: 26rpx;
  color: #b2b2b2;
}
.qingful-bbs-container .userList .user .btm .operateTab .icon {
  width: 40rpx;
  height: 28rpx;
}
