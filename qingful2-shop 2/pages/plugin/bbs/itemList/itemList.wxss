.qingful-bbs-container {
  position: relative;
  width: 100%;
  height: 100%;
  background: #e6e6e6;
  font-family: "PingFangSC-Regular";
}
.qingful-bbs-container .userList {
  position: absolute;
  top: 20rpx;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
}
.qingful-bbs-container .userList .user {
  width: 100%;
  height: 200rpx;
  background: #fff;
  box-sizing: border-box;
  padding: 20rpx 24rpx;
  border: 1rpx solid #e6e6e6;
}
.qingful-bbs-container .userList .user view {
  display: inline-block;
  vertical-align: top;
}
.qingful-bbs-container .userList .user image.userPhoto {
  width: 160rpx;
  height: 160rpx;
  border-radius: 10rpx;
  background: lightblue;
}
.qingful-bbs-container .userList .user view:nth-child(2) {
  width: 494rpx;
  vertical-align: top;
  margin-left: 34rpx;
}
.qingful-bbs-container .userList .user view:nth-child(2) view:nth-child(1) {
  font-size: 34rpx ;
  line-height: 48rpx;
}
.qingful-bbs-container .userList .user view:nth-child(2) view:nth-child(2) {
  margin-left: 0;
  color: #8a8a8a;
}
.qingful-bbs-container .userList .user view:nth-child(2) view:nth-child(2) text {
  display: inline-block;
  line-height: 38rpx;
  margin-top: 10rpx;
}
