@font-face {
    font-family: 'iconfont';
    /* project id 560268 */
    src: url('//at.alicdn.com/t/font_560268_fob2qv2c33plow29.eot');
    src: url('//at.alicdn.com/t/font_560268_fob2qv2c33plow29.eot?#iefix') format('embedded-opentype'),
    url('//at.alicdn.com/t/font_560268_fob2qv2c33plow29.woff') format('woff'),
    url('//at.alicdn.com/t/font_560268_fob2qv2c33plow29.ttf') format('truetype'),
    url('//at.alicdn.com/t/font_560268_fob2qv2c33plow29.svg#iconfont') format('svg');
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-address:before {
    content: "\e6f3";
}

.icon-yanjing:before {
    content: "\e70c";
}

.icon-pinglun:before {
    content: "\e608";
}

.icon-zan:before {
    content: "\e6a7";
}

.icon-fenxiang:before {
    content: "\e6a4";
}

.icon-xihuan:before {
    content: "\e663";
}

.icon-xihuan-xuanzhong:before {
    content: "\e707";
