.hidden-button{
  opacity: 0;
  z-index: 1;
  position:absolute;
  top:0;
  right:0;
  border-radius: 0;
  width: 100%;
  height: 100%;
}

.qingful-bbs-container {
  position: relative;
  width: 100%;
  height: 100%;
  background: #e6e6e6;
  box-sizing: border-box;
  padding-top: 20rpx;
  font-family: "PingFangSC-Regular";
}
.qingful-bbs-container image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.qingful-bbs-container .user {
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  padding: 0rpx 24rpx;
  border-bottom: 1rpx solid #e6e6e6;
}
.qingful-bbs-container .user .name {
  position: relative;
  vertical-align: middle;
  padding-top: 20rpx;
  height: 110rpx;
}
.qingful-bbs-container .user .name view {
  display: inline-block;
  color: #222;
}
.qingful-bbs-container .user .name view:nth-child(2) {
  width: 210rpx;
  height: 100%;
  vertical-align: top;
}
.qingful-bbs-container .user .name view:nth-child(2) text {
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
  line-height: 42rpx;
}
.qingful-bbs-container .user .name view:nth-child(2) text:nth-child(2) {
  font-size: 26rpx;
  color: #8a8a8a;
  line-height: 38rpx;
}
.qingful-bbs-container .user .name .photo {
  width: 90rpx;
  height: 90rpx;
  margin-right: 20rpx;
  background: lightgray;
  border-radius: 50%;
}
.qingful-bbs-container .user .name .concat {
  position: absolute;
  top: 50%;
  right: 24rpx;
  transform: translateY(-50%);
  width: 120rpx;
  height: 56rpx;
  border: 2rpx solid #ff5757;
  text-align: center;
  line-height: 56rpx;
  font-size: 26rpx;
  border-radius: 6rpx;
  color: #ff5757;
}
.qingful-bbs-container .user .title {
  width: 100%;
  height: 46rpx;
}
.qingful-bbs-container .user .title text {
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
}
.title-tap-1 {
  padding: 2rpx;
  width: 32rpx;
  height: 32rpx;
  border-radius: 6rpx;
  font-size: 22rpx;
  background-color: #ff5757;
  color: #fff;
  line-height: 32rpx;
  margin-right: 2rpx;
}
.title-tap-2 {
  padding: 2rpx;
  width: 32rpx;
  height: 32rpx;
  font-size: 22rpx;
  border-radius: 6rpx;
  background-color: #08b7fc;
  color: #fff;
  line-height: 32rpx;
  margin-right: 14rpx;
}
.qingful-bbs-container .user .con {
  margin-top: 20rpx;
}
.qingful-bbs-container .user .con view {
  color:#8a8a8a;
  font-size: 28rpx;
}
.qingful-bbs-container .user .con view:nth-child(1) {
  line-height: 40rpx;
  display: -webkit-box;
  overflow: hidden;
  white-space: normal!important;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.qingful-bbs-container .user .con .imgList {
  width: 100%;
  display: flex;
  height: 220rpx;
  margin: 20rpx 0rpx;
  box-sizing: border-box;
}
.qingful-bbs-container .user .con .imgList .imgWrapper {
  display: block;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size: 0;
  text-align: center;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
.qingful-bbs-container .user .con .imgList .imgWrapper image {
  width: 220rpx;
  height: 220rpx;
  background-color: #8a8a8a;
}
.qingful-bbs-container .user .con .tagList {
  margin-bottom: 20rpx;
}
.qingful-bbs-container .user .con .tagList text {
  margin-right: 20rpx;
  line-height: 48rpx;
  font-size: 26rpx;
  background-color: #efefef;
  padding: 6rpx 12rpx;
  color: #999;
  border-radius: 6rpx;
}
.qingful-bbs-container .user .con .position {
  position: relative;
  width: 100%;
  color: #8a8a8a;
  font-size: 26rpx;
  vertical-align: middle;
}
.qingful-bbs-container .user .con .position .icon {
  font-size: 44rpx;
}
.qingful-bbs-container .user .con .position text {
    display: inline-block;
    font-size: 26rpx;
    line-height: 38rpx;
    color: #999;
    vertical-align: middle;
}
.qingful-bbs-container .user .con .position text:nth-last-child(1) {
  position: absolute;
  right: 24rpx;
}
.qingful-bbs-container .user .btm {
  display: flex;
}
.qingful-bbs-container .user .btm .operateTab {
  flex: 1;
  text-align: center;
  padding: 30rpx 0rpx;
  position: relative;
}
.qingful-bbs-container .user .btm .operateTab text {
  display: inline-block;
  margin-right: 12rpx;
  font-size: 26rpx;
  color: #b2b2b2;
}
.qingful-bbs-container .user .btm .operateTab .icon {
    font-size: 30rpx;
    vertical-align: middle;
}
.qingful-bbs-container .commentPart {
  background: #fff;
  position: relative;
  width: 100%;
}
.qingful-bbs-container .commentPart .all {
  box-sizing: border-box;
  padding-left: 24rpx;
  font-size: 34rpx;
  font-weight: bold;
  font-stretch: normal;
  line-height: 94rpx;
  color: #222222;
  background: #fff;
}
.qingful-bbs-container .commentPart .content {
  width: 100%;
  box-sizing: border-box;
  background: #fff;
}
.qingful-bbs-container .commentPart .content .specCon {
  width: 100%;
  box-sizing: border-box;
  padding-left: 24rpx;
  padding-right: 24rpx;
}
.qingful-bbs-container .commentPart .content .specCon .photo {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  background-color: #E5E5E5;
  display: inline-block;
  float: left;
}
.qingful-bbs-container .commentPart .content .specCon .comment {
  padding-left: 88rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  width: 100%;
}
.qingful-bbs-container .commentPart .content .specCon .comment .nickName {
  position: relative;
  font-size: 30rpx;
  font-stretch: normal;
  line-height: 50rpx;
  color: #606f89;
  margin-bottom: 7rpx;
  font-weight: bold;
}
.qingful-bbs-container .commentPart .content .specCon .comment .nickName .support {
  position: absolute;
  right: 24rpx;
  font-size: 26rpx;
  color: #b2b2b2;
}
.qingful-bbs-container .commentPart .content .specCon .comment .nickName .support::before {
  position: absolute;
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  top: 10rpx;
  left: -40rpx;
  content: '';
  background: lightgray;
}
.qingful-bbs-container .commentPart .content .specCon .comment .con {
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 50rpx;
  color: #222222;
}
.qingful-bbs-container .commentPart .content .specCon .comment .info .date {
  font-size: 26rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 40rpx;
  color: #8a8a8a;
}
.qingful-bbs-container .commentPart .content .specCon .comment .info .num {
  font-size: 26rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 40rpx;
  color: #222222;
  float: right;
}
.qingful-bbs-container .commentPart .content .specCon .comment .info .num:hover {
  color: #606f89;
  font-family: PingFangSC-Regular;
  font-weight: bold;
}
.qingful-bbs-container .commentPart .content .specCon .replay {
  margin-left: 88rpx;
  margin-bottom: 20rpx;
  background-color: #f8f8f8;
  padding: 10rpx 10rpx;
}
.qingful-bbs-container .commentPart .content .specCon .replay .title {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  font-stretch: normal;
  line-height: 50rpx;
  color: #606f89;
  margin-bottom: 7rpx;
  font-weight: bold;
}
.qingful-bbs-container .commentPart .content .specCon .replay view {
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 50rpx;
  color: #222222;
}
.qingful-bbs-container .commentPart .operate {
  position: absolute;
  box-sizing: border-box;
  left: 0;
  right: 0;
  height: 98rpx;
  border-top: 1px solid #e6e6e6 ;
  padding: 16rpx 0rpx 16rpx 24rpx;
}
.qingful-bbs-container .commentPart .operate input {
  height: 66rpx;
  background-color: #f8f8f8;
  border-radius: 10rpx;
  width: 606rpx;
  padding-left: 10rpx;
}
.qingful-bbs-container .commentPart .operate .commentStyle {
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 40rpx;
  color: #8a8a8a;
}
.qingful-bbs-container .commentPart .operate button {
  position: absolute;
  box-sizing: border-box;
  right: 0rpx;
  top: 0rpx;
  height: 100%;
  font-size: 28rpx;
  color: #222222;
  padding: 29rpx 24rpx;
  line-height: 45rpx;
  background-color: #fff;
}
.qingful-bbs-container .commentPart .operate button::after {
  border: none;
}
.qingful-article-comment-bottom {
    position: fixed;
    height: 50px;
    border-top: 1rpx solid #e6e6e6;
    bottom: 0px;
    width: 100%;
    display: flex;
    align-items: center;
    background-color: #ffffff;
}

.qingful-article-comment-bottom input {
    background-color: #f8f8f8;
    border-radius: 10px;
    padding: 8rpx 6rpx;
    width: 100%;
    margin-left: 10px;
}

.qingful-article-comment-bottom .fabu {
    text-align: center;
    width: 60px;
    line-height: 50px;
}
