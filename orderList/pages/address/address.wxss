page {
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  overflow: auto;
  font-family: PingFangSC-Regular;
}
.container {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #e6e6e6;
  box-sizing: border-box;
  font-size: 30rpx;
  padding: 0rpx 6rpx 0rpx 24rpx;
}
.container image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.container view {
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
}
.container .city {
  display: inline-block;
  font-size: 28rpx;
  line-height: 60rpx;
  margin-right: 30rpx;
  background: #fff;
  color: #222;
  box-sizing: border-box;
  padding: 0rpx 33rpx;
  margin-top: 30rpx;
  border-radius: 100rpx;
  height: 60rpx;
  text-align: center;
}
